{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://auction-bidding-system-backend.vercel.app/api/$1"
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
